{
  "private": true,
  "name": "shared-commands",
  "version": "0.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "description": "",
  "type": "module",
  "scripts": {
    "bundle": "tsup-node",
    "bundle:watch": "tsup-node --watch",
    "type:check": "tsc --noEmit",
    "type:gen": "tsc --emitDeclarationOnly",
    "build": "run-p bundle type:gen",
    "clean": "del dist tsconfig.tsbuildinfo",
    "start": "node --env-file=.env dist/index.js",
    "dev": "tsup --watch --onSuccess 'node --env-file=.env dist/main.js'",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "check": "tsc --noEmit",
    "test": "tap"
  },
  "keywords": [
    "discord",
    "discord-bot",
    "fp",
    "fp-ts"
  ],
  "author": "cweiqi27",
  "license": "MIT",
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.9.4",
    "tap": "^18.6.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "del-cli": "^5.1.0",
    "discord.js": "^14.14.1",
    "npm-run-all": "^4.1.5",
    "shared-configs": "workspace:*",
    "shared-types": "workspace:*"
  }
}
